  "description": "Backend for Hire BandB - bike rental",
  "main": "dist/index.js",
  "type": "commonjs",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "dev:nodemon": "nodemon",
